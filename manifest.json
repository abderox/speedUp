{
    "name": "SpeedUp",
    "version": "1.0.4.0",
    "description": "SpeedUp {author : 'ABDELHADI MOUZAFIR'}",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
      }
}